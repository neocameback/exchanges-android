<?xml version="1.0" encoding="utf-8"?>
<resources>

  <declare-styleable name="ScrollLayout">
    <attr name="labelerClass" format="string"/>
    <attr name="labelerFormat" format="string"/>
    <attr name="childWidth" format="dimension"/>
    <attr name="childHeight" format="dimension"/>
  </declare-styleable>
  <declare-styleable name="CircularImageView">
    <attr name="border" format="boolean"></attr>
    <attr name="border_width" format="dimension"></attr>
    <attr name="border_color" format="color"></attr>
    <attr name="shadow" format="boolean"></attr>
  </declare-styleable>
  <declare-styleable name="Theme">
    <attr name="circularImageViewStyle" format="reference"></attr>
  </declare-styleable>

  <declare-styleable name="CircularProgressBar">
    <attr name="cpbStyle" format="reference"/>
    <attr name="cpb_color" format="color"/>
    <attr name="cpb_colors" format="reference"/>
    <attr name="cpb_stroke_width" format="dimension"/>
    <attr name="cpb_min_sweep_angle" format="integer"/>
    <attr name="cpb_max_sweep_angle" format="integer"/>
    <attr name="cpb_sweep_speed" format="float"/>
    <attr name="cpb_rotation_speed" format="float"/>
  </declare-styleable>

</resources>